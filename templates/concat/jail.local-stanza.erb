[<%= @real_jailname %>]
enabled  = <%= @real_status %>
<% if @real_filter != '' -%>
filter   = <%= @real_filter %>
<% end -%>
<% if @array_port != [] -%>
port     = <%= @array_port * ',' %>
<% end -%>
<% if @array_action != [] -%>
action   = <% @array_action.each do |a| -%>
          <%= a %>
          <% end -%>
<% end -%>
<% if @real_logpath != '' -%>
logpath  = <%= @real_logpath %>
<% end -%>
<% if @real_maxretry != '' -%>
maxretry = <%= @real_maxretry %>
<% end -%>
<% if @real_bantime != '' -%>
bantime  = <%= @real_bantime %>
<% end -%>
<% if @findtime != '' -%>
findtime = <%= @findtime %>
<% end -%>

